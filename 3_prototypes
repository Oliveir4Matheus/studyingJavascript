// [ O QUE É PROTOTYPES ]
// Prototypes é um recurso do javascript que permite que objetos possam herdar propriedades e métodos de outro objeto. 

function info(name,size,weight,value){
    this.name = name,
    this.size = size,
    this.weight = weight,
    this.value = value;
} // metodo construtor

let computer = new info("HP 840 G3","35cm","1.25KG",25); 
// computer é um objeto que herda propriedades metodos do método construtor info.
console.log(Object.getPrototypeOf(computer))